<!--	## car3-trb1 Mass: 1150Kg, Power: 384Kw, Cx*A: 0.619m2, Fuel: 83kg  -->

<!DOCTYPE params SYSTEM "../../../../src/libs/tgf/params.dtd">

<params name="car4-trb1" type="template" mode="mw">
	
    <!-- Fuel 
	<section name="Car">
		<attnum name="initial fuel" unit="l" min="1.0" max="83.0" val="80.0"/>
	</section>
-->
    <!-- Wings -->
  <section name="Front Wing">
    <attnum name="angle" min="0" max="12" unit="deg" val="12.0"/>
  </section>
  <section name="Rear Wing">
    <attnum name="angle" min="0" max="17" unit="deg" val="8.5"/>
  </section>

	<section name="Gearbox">
		<attnum name="shift time" unit="s" val="0.15"/>
		<section name="gears">
			<section name="r">
				<attnum name="ratio" min="-6" max="-3" val="-4.0"/>
			</section>
			<section name="1">
				<attnum name="ratio" min="0.5" max="5" val="3.9"/>
			</section>
			<section name="2">
				<attnum name="ratio" min="0.5" max="5" val="2.50"/>
			</section>
			<section name="3">
				<attnum name="ratio" min="0.5" max="5" val="1.90"/>
			</section>
			<section name="4">
				<attnum name="ratio" min="0.5" max="5" val="1.50"/>
			</section>
			<section name="5">
				<attnum name="ratio" min="0.5" max="5" val="1.30"/>
			</section>
			<section name="6">
				<attnum name="ratio" min="0.5" max="5" val="1.20"/>
			</section>
		</section>
	</section>

    <!-- Differentials -->
  <section name="Rear Differential">
    <attnum name="ratio" min="1" max="10" val="3.185"/>
    <attnum name="max slip bias" min="0.00" max="0.5" val="0.175"/>
    <attnum name="locking input torque" min="500" max="90000" val="2500"/>
    <attnum name="locking brake input torque" min="150" max="90000" val="150"/>
  </section>

    <!-- Brakes system -->
	<section name="Brake System">
		<attnum name="front-rear brake repartition" min="0.3" max="0.7" val="0.450"/>
		<attnum name="max pressure" unit="kPa" val="40000"/>
	</section>

    <!-- Rollbars -->
  <section name="Front Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="5000"/>
  </section>
  <section name="Rear Anti-Roll Bar">
    <attnum name="spring" min="0" max="5000" unit="lbs/in" val="4000"/>
  </section>

    <!-- Wheels -->
	<section name="Front Right Wheel">
		<attnum name="ride height" unit="mm" min="70" max="120" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="-0.1"/>
		<attnum name="camber" min="-5" max="-3" unit="deg" val="-5" />
	</section>
	<section name="Front Left Wheel">
		<attnum name="ride height" unit="mm" min="70" max="120" val="70"/>
		<attnum name="toe" unit="deg" min="-5" max="5" val="0.1"/>
		<attnum name="camber" min="-5" max="-3" unit="deg" val="-5"/>
	</section>
	
	<section name="Rear Right Wheel">
		<attnum name="ride height" unit="mm" min="70" max="120" val="70"/>
		<attnum name="toe" unit="deg" min="0" max="0" val="0"/>
		<attnum name="camber" min="-5" max="-2" unit="deg" val="-5"/>
	</section>
	<section name="Rear Left Wheel">
		<attnum name="ride height" unit="mm" min="70" max="120" val="70"/>
		<attnum name="toe" unit="deg" min="0" max="0" val="0"/>
		<attnum name="camber" min="-5" max="-2" unit="deg" val="-5"/>
	</section>

  <section name="Front Axle">
    	<attnum name="spring" min="0" max="40000" unit="lbs/in" val="40000"/>
    	<attnum name="suspension course" min="0" max="60" unit="mm" val="15"/>
    	<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="1000"/>
    	<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="1000"/>
  </section>
  <section name="Rear Axle">
    	<attnum name="spring" min="0" max="40000" unit="lbs/in" val="40000"/>
    	<attnum name="suspension course" min="0" max="60" unit="mm" val="10"/>
    	<attnum name="slow bump" min="0" max="1000" unit="lbs/in/s" val="1000"/>
    	<attnum name="slow rebound" min="0" max="1000" unit="lbs/in/s" val="1000"/>
  </section>

    <!-- Suspensions -->
  <section name="Front Right Suspension">
    	<attnum name="spring" min="500" max="10000" unit="lbs/in" val="500"/>
    	<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    	<attnum name="packers" min="0" max="30" unit="mm" val="0"/>
    	<attnum name="slow bump" min="50" max="1000" unit="lbs/in/s" val="300"/>
    	<attnum name="slow rebound" min="50" max="1000" unit="lbs/in/s" val="300"/>
    	<attnum name="fast bump" min="10" max="200" unit="lbs/in/s" val="100"/>
    	<attnum name="fast rebound" min="10" max="200" unit="lbs/in/s" val="40"/>
  </section>
  <section name="Front Left Suspension">
    	<attnum name="spring" min="500" max="10000" unit="lbs/in" val="500"/>
    	<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    	<attnum name="packers" min="0" max="30" unit="mm" val="0"/>
    	<attnum name="slow bump" min="50" max="1000" unit="lbs/in/s" val="300"/>
    	<attnum name="slow rebound" min="50" max="1000" unit="lbs/in/s" val="300"/>
    	<attnum name="fast bump" min="10" max="200" unit="lbs/in/s" val="100"/>
    	<attnum name="fast rebound" min="10" max="200" unit="lbs/in/s" val="40"/>
  </section>

  <section name="Rear Right Suspension">
    	<attnum name="spring" min="500" max="10000" unit="lbs/in" val="7000"/>
    	<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    	<attnum name="packers" min="0" max="30" unit="mm" val="0"/>
    	<attnum name="slow bump" min="50" max="1000" unit="lbs/in/s" val="300"/>
    	<attnum name="slow rebound" min="50" max="1000" unit="lbs/in/s" val="300"/>
    	<attnum name="fast bump" min="10" max="200" unit="lbs/in/s" val="80"/>
    	<attnum name="fast rebound" min="10" max="200" unit="lbs/in/s" val="40"/>
  </section>
  <section name="Rear Left Suspension">
    	<attnum name="spring" min="500" max="10000" unit="lbs/in" val="7000"/>
    	<attnum name="suspension course" min="0" max="0.2" unit="m" val="0.2"/>
    	<attnum name="packers" min="0" max="30" unit="mm" val="0"/>
    	<attnum name="slow bump" min="50" max="1000" unit="lbs/in/s" val="300"/>
    	<attnum name="slow rebound" min="50" max="1000" unit="lbs/in/s" val="300"/>
    	<attnum name="fast bump" min="10" max="200" unit="lbs/in/s" val="80"/>
    	<attnum name="fast rebound" min="10" max="200" unit="lbs/in/s" val="40"/>
  </section>
	
    <!-- Brake -->

