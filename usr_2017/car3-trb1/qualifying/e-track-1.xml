     		<attnum name="speedadjust" min="-5.0" max="5.0" val="4.0" />
		<attnum name="steercorrect" min="0.0" max="2.0" val="0.5"/>
		<attnum name="hold mid distance" val="410" />
		<attnum name="steer skid" val="1.25"/>
		<attnum name="steer skid offline" val="1.4"/>
		<attnum name="brake coefficient" val = "1.500"/>
		<attnum name="brake multiplier" val = "0.40"/>
